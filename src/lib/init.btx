% ==========          COUNTERS          ==========

\def\@inc#1{\@@add{#1}{1}}

\def\@newcounter#1{%
    \@@set{g.ctr-#1}{0}%
    \@defbyname{the#1}{\arabic{#1}}%
    \@defbyname{@next#1}{}%
}

