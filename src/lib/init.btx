\def\theoremstyle#1{%
    \@@if{#1}{plain}{%
        \@changethmbegin{\@thmbegina}%
    }{\@@if{#1}{definition}{%
        \@changethmbegin{\@thmbeginb}%
    }{\@@if{#1}{remark}{%
        \@changethmbegin{\@thmbeginc}%
    }{\@@throw{UNKNOWN_THEOREM_STYLE}}}}%
}

\def\@changethmbegin#1{%
    \@@def\newtheorem##1##2{%
        \@envdef{##1}{}{%
            #1%
            \begin{@theorem}{%
                \refstepcounter{theorem}%
                ##2 \thetheorem%
            }%
        }{\end{@theorem}}%
    }%
    \@pdef\newtheorem*##1##2{%
        \@envdef{##1}{}{%
            #1%
            \begin{@theorem}{##2}%
        }{\end{@theorem}}%
    }%
}

