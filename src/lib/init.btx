            \@@throw{UNMATCHED_ENVIRONMENT}%
        }%
    }{\@@text{\end}{#1}}%
    \@endgroup%
}

\def\@envdef#1#2#3#4{%
    \@settrue{env-defined-#1}%
    \@defbyname{env.#1.begin}#2{#3}%
    \@defbyname{env.#1.end}{#4}%
}
