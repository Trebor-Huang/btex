}

\def\stepcounter#1{%
    \@inc{g.ctr-#1}%
    \@@cmd{@next#1}%
}

\def\refstepcounter#1{%
    \@inc{g.ctr-#1}%
    \@@cmd{@next#1}%
    \@edef\@currentlabel{\@@cmd{the#1}}%
    \@@bmk%
}

\def\addtocounter#1#2{%
    \@@add{g.ctr-#1}{#2}%
    \@@cmd{@next#1}%
}

\def\setcounter#1#2{%
    \@@set{g.ctr-#1}{#2}%
    \@@cmd{@next#1}%
}

\def\@addtoreset#1#2{%
    \@patchbyname{@next#2}{%
        \setcounter{#1}{0}%
    }%
}

\def\numberwithin#1#2{%
    \@addtoreset{#1}{#2}%
    \@defbyname{the#1}{%
        \@@cmd{the#2}.\arabic{#1}%
    }%
}

\def\arabic#1{%
    \@@set{get-format}{0}%
